{
    "name": "ghci-helper",
    "displayName": "GHCi Helper",
    "description": "Helpers for GHCi and Haskell Stack",
    "author": "Richard Cook",
    "license": "MIT",
    "version": "0.0.5",
    "publisher": "rcook",
    "homepage": "https://github.com/rcook/ghci-helper",
    "repository": {
        "type": "git",
        "url": "https://github.com/rcook/ghci-helper.git"
    },
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "ghci",
        "haskell",
        "stack"
    ],
    "activationEvents": [
        "onCommand:extension.ghciHelperStart",
        "onCommand:extension.ghciHelperReload",
        "onCommand:extension.ghciHelperMain"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.ghciHelperStart",
                "title": "ghci-helper: (ghciHelperStart) Start GHCi using Stack"
            },
            {
                "command": "extension.ghciHelperReload",
                "title": "ghci-helper: (ghciHelperReload) send :reload to terminal"
            },
            {
                "command": "extension.ghciHelperMain",
                "title": "ghci-helper: (ghciHelperMain) send :main to terminal"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
